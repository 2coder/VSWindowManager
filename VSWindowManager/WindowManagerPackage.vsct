      <Parent guid="guidWindowManagerPackageCmdSet" id="RecentWindowsGroup" />
    </CommandPlacement>
    <CommandPlacement guid="guidWindowManagerPackageCmdSet" id="RecentToolWindow7" priority="0x0300">
