    </CommandPlacement>
    <CommandPlacement guid="guidWindowManagerPackageCmdSet" id="RecentToolWindow9" priority="0x0100">
      <Parent guid="guidWindowManagerPackageCmdSet" id="RecentWindowsGroup" />
